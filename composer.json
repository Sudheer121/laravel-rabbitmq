{
    "name": "kunalvarma05/laravel-rabbitmq",
    "description": "Work with RabbitMQ in Laravel.",
    "keywords": [
        "amqp",
        "laravel",
        "rabbitmq"
    ],
    "homepage": "https://github.com/kunalvarma05/laravel-rabbitmq",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunal Varma",
            "email": "kunalvarma05@gmail.com",
            "homepage": "https://kunalvarma.in",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.5.5|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "~3.5.3|~3.6.0|~3.7.0|~3.8.0|^4.0",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Kunnu\\RabbitMQ\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kunnu\\RabbitMQ\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "phpunit -v --coverage-text --colors=never --stderr",
        "lint": "phpcs -n",
        "lint-fix": "php-cs-fixer fix && phpcbf"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kunnu\\RabbitMQ\\RabbitMQServiceProvider"
            ]
        }
    }
}
